{
    "hud_actionbar_text": {
        "$abt": "$actionbar_text",
        "visible": "(($abt - 'Thirst') = $abt)"
    },
    "thirst_actionbar_text": {
        "type": "label",
        "anchor_from": "bottom_right",
        "anchor_to": "bottom_right",
        "color": "$tool_tip_text",
        "layer": 31,
        "text": "$actionbar_text",
        "localize": false,
        "offset": [
            -4,
            -4
        ],
        "$abt": "$actionbar_text",
        "visible": "(not (($abt - 'Thirst') = $abt) )"
    },
    "thirst_actionbar": {
        "type": "panel",
        "factory": {
        "name": "hud_actionbar_text_factory",
        "control_ids": {
            "hud_actionbar_text": "thirst_actionbar@hud.thirst_actionbar_text"
        }
        }
    },
    "root_panel": {
        "modifications": [
            {
                "array_name": "controls",
                "operation": "insert_back",
                "value": {
                    "thirst_actionbar@hud.thirst_actionbar": {}
                }
            }
        ]
    }
}